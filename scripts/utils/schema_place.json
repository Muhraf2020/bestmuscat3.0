{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Place",
  "type": "object",
  "required": [
    "id",
    "slug",
    "name",
    "categories",
    "location"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "pattern": "^[a-z0-9-]+$"
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1
    },
    "subcategories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "badges": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "price_range": {
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string"
        },
        "min": {
          "type": "number"
        },
        "max": {
          "type": "number"
        },
        "currency": {
          "type": "string"
        }
      }
    },
    "cuisines": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "meals": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "dishes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "amenities": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "attributes": {
      "type": "object",
      "additionalProperties": {
        "type": [
          "boolean",
          "number",
          "string"
        ]
      }
    },
    "rating_overall": {
      "type": "number"
    },
    "subscores": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    },
    "status_labels": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "hours": {
      "type": "object"
    },
    "busy_profile": {
      "type": "object"
    },
    "location": {
      "type": "object",
      "required": [
        "lat",
        "lng",
        "address"
      ],
      "properties": {
        "lat": {
          "type": "number"
        },
        "lng": {
          "type": "number"
        },
        "neighborhood": {
          "type": [
            "string",
            "null"
          ]
        },
        "address": {
          "type": "string"
        },
        "plus_code": {
          "type": [
            "string",
            "null"
          ]
        },
        "what3words": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "landmarks": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "actions": {
      "type": "object",
      "properties": {
        "website": {
          "type": [
            "string",
            "null"
          ]
        },
        "phone": {
          "type": [
            "string",
            "null"
          ]
        },
        "maps_url": {
          "type": [
            "string",
            "null"
          ]
        },
        "menu_url": {
          "type": [
            "string",
            "null"
          ]
        },
        "booking_url": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "gallery": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "about": {
      "type": "string"
    },
    "best_times": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "window": {
            "type": "string"
          }
        }
      }
    },
    "public_sentiment": {
      "type": "object",
      "properties": {
        "source": {
          "type": "string"
        },
        "count": {
          "type": "number"
        },
        "summary": {
          "type": "string"
        },
        "last_updated": {
          "type": "string"
        }
      }
    },
    "faqs": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "q": {
            "type": "string"
          },
          "a": {
            "type": "string"
          }
        }
      }
    },
    "verified": {
      "type": "boolean"
    },
    "methodology_note": {
      "type": "string"
    },
    "provenance": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "last_updated": {
      "type": "string"
    }
  }
}